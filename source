	bv.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
	bv.Velocity = Vector3.new(0, power, 0)
	hypernull(function()
		bv.Parent = z.local_character.HumanoidRootPart
	end)

	z.debris:AddItem(bv, 0.5)
end)

