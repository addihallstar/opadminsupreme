		loadstring(game:HttpGet('https://raw.githubusercontent.com/InfernusScripts/Octo-Spy/refs/heads/main/Main.lua'))()
	end)
	if not success then
		notify('failed to load remotespy: ' .. tostring(err))
	end
end)

